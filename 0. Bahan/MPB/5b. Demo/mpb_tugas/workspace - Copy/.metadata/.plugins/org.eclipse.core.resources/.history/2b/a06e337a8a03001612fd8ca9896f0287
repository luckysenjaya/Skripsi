<html>

<head></head>

<body>
	<div class="results">
	<script>
		camForm.on('form-loaded', function() {
		  // fetch the variable named 'pemohon'
		  camForm.variableManager.fetchVariable('pemohon');
		});
		
		camForm.on('variables-fetched', function() {
		  // after the variables are fetched, bind the value of customerData to a angular
		  // scope value such that the form can work on it
		  $scope.pemohon = camForm.variableManager.variable('pemohon').value;
		  document.querySelector().innerHTML = 'Hello World!';
		});
		
		
		
		$('.results').html('')
	</script>
	</div>
</body>
</html>